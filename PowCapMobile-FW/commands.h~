#ifndef _COMMANDS_H_
#define _COMMANDS_H

#include "FSM.h"

void cb_gainV1(char *param);	//voltage1 PGA
void cb_gainC1(char *param);	//current1 PGA
void cb_gainV2(char *param);	//voltage2 PGA
void cb_gainC2(char *param);	//current2 PGA

void cb_filterV1(char *param); //voltage filters
void cb_filterC1(char *param); //current filters
void cb_filterV2(char *param); //voltage filters
void cb_filterC2(char *param); //current filters

void cb_notchV1(char *param); //voltage 60hz notch filter
void cb_notchC1(char *param); //current 60hz notch filter
void cb_notchV2(char *param); //voltage2 60hz notch filter
void cb_notchC2(char *param); //current2 60hz notch filter

#endif
